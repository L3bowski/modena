{
  "name": "modena",
  "version": "1.3.0",
  "description": "Express based web server with support for multiple isolated apps",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "scripts": {
    "lint": "tslint -e 'node_modules/**' -p .",
    "test": "nyc mocha -r ts-node/register lib/**/*.test.ts",
    "npm-publish": "tsc -d && npm publish"
  },
  "dependencies": {
    "body-parser": "^1.17.1",
    "ejs": "^2.5.6",
    "express": "^4.13.3",
    "express-asset-versions": "^0.1.2",
    "express-session": "^1.15.2",
    "http": "0.0.0",
    "https": "^1.0.0",
    "node-sass": "^4.5.0",
    "passport": "^0.3.2",
    "passport-local": "^1.0.0",
    "winston": "^2.4.4"
  },
  "devDependencies": {
    "@types/chai": "^4.1.4",
    "@types/express": "^4.16.0",
    "@types/express-session": "^1.15.10",
    "@types/mocha": "^5.2.5",
    "@types/mock-require": "^2.0.0",
    "@types/node": "^10.5.7",
    "@types/node-sass": "^3.10.32",
    "@types/passport": "^0.4.6",
    "@types/passport-local": "^1.0.33",
    "@types/sinon": "^5.0.2",
    "@types/sinon-chai": "^3.2.0",
    "chai": "^4.1.2",
    "mocha": "^5.2.0",
    "mock-require": "^3.0.2",
    "nyc": "^12.0.2",
    "sinon": "^6.3.3",
    "sinon-chai": "^3.2.0",
    "source-map-support": "^0.5.9",
    "ts-node": "^7.0.1",
    "tslint": "^5.11.0",
    "typescript": "^3.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/L3bowski/modena-v1.git"
  },
  "keywords": [
    "modena",
    "multitenancy",
    "express",
    "multiple",
    "apps",
    "web",
    "server"
  ],
  "author": "Carles Capellas <capellas.carles@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/L3bowski/modena-v1/issues"
  },
  "homepage": "https://github.com/L3bowski/modena-v1#readme",
  "nyc": {
    "extension": [
      ".ts"
    ],
    "include": "lib",
    "exclude": [
      "lib/types.ts",
      "lib/test.utils.ts",
      "**/*.d.ts",
      "**/*.test.ts"
    ],
    "reporter": [
      "text"
    ],
    "all": true
  }
}
